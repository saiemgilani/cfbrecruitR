cfbrecruits_starbin <- cfbrecruits %>%
filter(year < 2021,
!is.na(ranking),
ranking < 2000) %>%
group_by(stars) %>%
summarize(avg_rating = mean(rating),
is_stars = sum(instate),
b_stars = sum(bstate),
oor_stars = sum(oorstate),
is_stars_pct = mean(instate),
b_stars_pct = mean(bstate),
oor_stars_pct = mean(oorstate)) %>%
ungroup()
cfbrecruits_starbin5yr <- cfbrecruits %>%
filter(year < 2021,
year>= 2016,
!is.na(ranking),
ranking < 2000) %>%
group_by(stars) %>%
summarize(avg_rating = mean(rating),
is_stars = sum(instate),
b_stars = sum(bstate),
oor_stars = sum(oorstate),
is_stars_pct = mean(instate),
b_stars_pct = mean(bstate),
oor_stars_pct = mean(oorstate)) %>%
ungroup()
cfbrecruits_agg <- cfbrecruits %>%
left_join(cfbrecruits_starbin)
cfbrecruits_statestarbin <- cfbrecruits_agg %>%
filter(year < 2021,
!is.na(ranking),
ranking < 2000) %>%
group_by(stateProvince, stars) %>%
summarize(state_avg_rating = mean(rating),
state_is_stars = sum(instate),
state_b_stars = sum(bstate),
state_oor_stars = sum(oorstate),
state_is_stars_pct = mean(instate),
state_b_stars_pct = mean(bstate),
state_oor_stars_pct = mean(oorstate)) %>%
ungroup()
cfbrecruits_agg <- cfbrecruits_agg %>%
left_join(cfbrecruits_statestarbin)
